  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-dev": "dotenv -e .env.development react-scripts build",
    "build-prod": "dotenv -e .env.production react-scripts build",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
